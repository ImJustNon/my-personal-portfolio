<!-- add visitor count -->
<script>
  axios.get('/api/visitor-count/add')
  .then(function(response){
    let data = response.data;
    if(data.status == "SUCCESS"){
      console.log("[Debug] Added visitor count : " + data.data.count);
    }
    else {
      console.log("[FAIL] Fail to add visitor count [1]");
    }
  })
  .catch(function(error){
    console.log("[FAIL] Fail to add visitor count [2]");
  });
</script>