<!-- Axios script -->
<script src="libs/axios/dist/axios.min.js"></script>
<!-- Socket.io script -->
<script src="libs/socket.io-client/dist/socket.io.min.js"></script>
<!-- Socket.io setup -->
<script>
  const socket = io('http://127.0.0.1:8080', {
    transports: ['websocket']
  });
  socket.on("connect", () => {
    console.log("[Socket.io] Socket.io connected ID : " + socket.id);
  });
  socket.on("disconnect", () => {
    console.log("[Socket.io] Socket.io disconnected ID : " + socket.id);
  });
</script>
