<!-- add visitor count -->
<script>
  axios.get('/api/visitor-count/ad').then(function(response){
    let data = response.data;
    if(data.status == "SUCCESS"){
      console.log("[Visitor-count] Added visitor count : " + data.data.count);
    }
    else {
      console.log("[Visitor-count] Fail to add visitor count");
    }
  }).catch(function(error){
    console.log("[Visitor-count] Can't fetch from this url (visitor-count)");
  });
</script>
